{
  "name": "rpdiscordts",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@databases/pg": "^5.4.1",
    "@discordjs/builders": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@rimbu/typical": "^0.2.2",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^17.0.21",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "debug": "^4.3.3",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.2.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "fp-ts": "^2.11.5",
    "fuzzysort": "^1.2.1",
    "monocle-ts": "^2.3.11",
    "rambda": "^6.9.0",
    "rxjs": "^7.5.5",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.5.2",
    "typescript-parsec": "^0.3.2",
    "url-regex-safe": "^2.0.2",
    "yargs": "^17.2.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@databases/pg-schema-cli": "^4.3.0",
    "@databases/pg-typed": "^4.3.0",
    "@types/jest": "^27.0.2",
    "@types/url-regex-safe": "^1.0.0",
    "@types/yargs": "^17.0.4",
    "fast-check": "^2.19.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ynpx": "^2.1.2"
  },
  "scripts": {
    "deploy-commands": "yarn tsc; yarn node dist/deploy-commands.js",
    "generate-server-sql-single-schema": "yarn run ynpx @databases/pg-schema-cli --config ./pg-config.json --database $SERVER_DATABASE_URL",
    "generate-server-dnd-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/dnd_5e --schemaName dnd_5e",
    "generate-server-public-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/public",
    "generate-server-pta-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/pta_03 --schemaName pta_03",
    "generate-server-ptu-05-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/ptu_05 --schemaName ptu_05",
    "generate-server-ptu-pt-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/ptu_pt --schemaName ptu_pt",
    "generate-server-ptu-al-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/ptu_al --schemaName ptu_al",
    "generate-server-ptu-ga-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/ptu_ga --schemaName ptu_ga",
    "generate-server-ptu-hi-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/ptu_hi --schemaName ptu_hi",
    "generate-server-zelda-schema": "yarn generate-server-sql-single-schema --directory src/generated_schema/server_data/zelda_rw --schemaName zelda_rw",
    "generate-server-all-schemas": "yarn generate-server-dnd-schema;yarn generate-server-public-schema;yarn generate-server-pta-schema;yarn generate-server-ptu-05-schema;yarn generate-server-ptu-pt-schema;yarn generate-server-ptu-al-schema;yarn generate-server-ptu-ga-schema;yarn generate-server-ptu-hi-schema;yarn generate-server-zelda-schema",
    "generate-user-sql-schema": "dotenv -- yarn run ynpx @databases/pg-schema-cli --directory src/generated_schema/user_data --config ./pg-config.json --database $USER_DATABASE_URL",
    "start": "yarn tsc; yarn node dist/index.js"
  }
}
